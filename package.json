{
  "name": "@owlprotocol/starter-react-vite",
  "version": "0.1.1",
  "private": true,
  "description": "Example React Component Library + App with Vite + Storybook",
  "keywords": [
    "pnpm",
    "vite",
    "react",
    "storybook"
  ],
  "license": "MIT",
  "author": "Leo Vigna",
  "scripts": {
    "build": "npm-run-all build:demo-components build:demo-app",
    "build:demo-app": "cd packages/demo-app && npm run build",
    "build:demo-components": "cd packages/demo-components && npm run build",
    "clean": "npm-run-all -p clean:lib clean:node_modules",
    "clean:lib": "rimraf packages/*/lib",
    "clean:node_modules": "rimraf packages/*/node_modules",
    "postinstall": "husky install",
    "publish": "pnpm publish -r"
  },
  "dependencies": {
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "@changesets/cli": "^2.21.1"
  }
}
